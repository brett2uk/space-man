{
  "name": "space-man",
  "displayName": "Space Man",
  "description": "Tree map of your project's space usage",
  "version": "1.0.0",
  "publisher": "SteveHankin",
  "engines": {
    "vscode": "^1.54.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stevenhankin/space-man.git"
  },
  "icon": "images/astronaut.png",
  "categories": [
    "Other"
  ],
  "keywords": [
    "directory",
    "size",
    "du",
    "vscode",
    "extension"
  ],
  "activationEvents": [
    "onView:spaceMan"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "spaceMan",
          "name": "Space Man"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.54.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "dir-roll-up": "^1.7.0",
    "directory-tree": "^2.2.7"
  }
}
